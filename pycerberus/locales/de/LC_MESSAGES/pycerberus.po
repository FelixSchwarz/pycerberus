#: pycerberus/api.py:291
msgid "Value must not be empty."
msgstr "Bitte geben Sie einen Wert ein."

#: pycerberus/schema.py:134
msgid ""
"if \"allow_additional_parameters\" is False, "
"\"filter_unvalidated_parameters=False\" is meaningless"
msgstr ""
"Wenn \"allow_additional_parameters\" False ist, wird die Angabe von "
"\"filter_unvalidated_parameters=False\" ignoriert"

#: pycerberus/schema.py:181
#, python-format
msgid "Validator got unexpected input (expected \"dict\", got \"%(classname)s\")."
msgstr ""
"Unerwartete Eingabe für Validator (\"dict\" erwartet, \"%(classname)s\" "
"erhalten)."

#: pycerberus/schema.py:182
#, python-format
msgid "Undeclared field detected: \"%(additional_item)s\"."
msgstr "Nicht deklariertes Feld gefunden: \"%(additional_item)s\"."

#: pycerberus/schemas.py:61
#, python-format
msgid "Unknown parameter \"%(additional_item)s\""
msgstr "Unbekannter Parameter \"%(additional_item)s\""

#: pycerberus/validators/basic_numbers.py:44
#: pycerberus/validators/foreach.py:51 pycerberus/validators/string.py:50
#, python-format
msgid "Validator got unexpected input (expected string, got \"%(classname)s\")."
msgstr ""
"Unerwartete Eingabe für Validator (String erwartet, \"%(classname)s\" "
"erhalten)."

#: pycerberus/validators/basic_numbers.py:45
msgid "Please enter a number."
msgstr "Bitte geben Sie eine Zahl ein."

#: pycerberus/validators/basic_numbers.py:46
#, python-format
msgid "Number must be %(min)d or greater."
msgstr "Die Zahl muss größer oder gleich %(min)d sein."

#: pycerberus/validators/basic_numbers.py:47
#, python-format
msgid "Number must be %(max)d or smaller."
msgstr "Die Zahl muss kleiner oder gleich %(max)d sein."

#: pycerberus/validators/checkbox.py:45
#, python-format
msgid "Value should be \"%s\" or \"%s\"."
msgstr "Der Wert sollte \"%s\" oder \"%s\" sein."

#: pycerberus/validators/checkbox.py:77
msgid "Please accept our Terms and Conditions."
msgstr "Bitte akzeptieren Sie unsere AGB."

#: pycerberus/validators/domain.py:38
#, python-format
msgid "Invalid character %(invalid_character)s in domain %(domain)s."
msgstr "Ungültiges Zeichen %(invalid_character)s in der Domain %(domain)s."

#: pycerberus/validators/domain.py:39
#, python-format
msgid "Invalid domain: %(domain)s must not start with a dot."
msgstr "Ungültige Domain: %(domain)s darf nicht mit einem Punkt beginnen."

#: pycerberus/validators/domain.py:40
#, python-format
msgid "Invalid domain: %(domain)s must not end with a dot."
msgstr "Ungültige Domain: %(domain)s darf nicht mit einem Punkt enden."

#: pycerberus/validators/domain.py:41
#, python-format
msgid "Invalid domain: %(domain)s must not contain consecutive dots."
msgstr ""
"Ungültige Domain: %(domain)s darf keine aufeinanderfolgenden Punkt "
"enthalten."

#: pycerberus/validators/email.py:58
msgid "An email address must contain a single '@'."
msgstr "Eine E-Mail-Adresse darf nur ein einziges '@'-Zeichen enthalten."

#: pycerberus/validators/email.py:59
#, python-format
msgid "Invalid character %(invalid_character)s in email address %(emailaddress)s."
msgstr ""
"Ungültiges Zeichen %(invalid_character)s in der E-Mail-Adresse "
"%(emailaddress)s."

#: pycerberus/validators/foreach.py:52
#, python-format
msgid "More than %(min)d items required."
msgstr "Es werden mehr als %(min)d Einträge benötigt."

#: pycerberus/validators/foreach.py:53
#, python-format
msgid "Less than %(max)d items required."
msgstr "Es werden weniger als %(max)d Einträge benötigt."

#: pycerberus/validators/matching_fields.py:39
msgid "Fields do not match"
msgstr "Die Felder stimmen nicht überein."

#: pycerberus/validators/oneof.py:39
msgid "This value is not allowed."
msgstr "Dieser Wert ist nicht erlaubt."

#: pycerberus/validators/string.py:51
#, python-format
msgid "Must be less than %(max)d characters long."
msgstr "Bitte geben Sie weniger als %(max)d Zeichen ein."

#: pycerberus/validators/string.py:52
#, python-format
msgid "Must be at least %(min)d characters long."
msgstr "Bitte geben Sie mindestens %(min)d Zeichen ein."

